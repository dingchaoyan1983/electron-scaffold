{
  "name": "Electron-Scaffold",
  "version": "1.0.0",
  "description": "My Electron Scaffold",
  "author": "Dane.Ding",
  "scripts": {
    "lint": "eslint ./",
    "build:renderer:dev": "better-npm-run build:renderer:dev",
    "build:renderer:prod": "better-npm-run build:renderer:prod",
    "build:main:dev": "cd app/ && npm install && cd .. && better-npm-run build:main:dev",
    "build:main:prod": "cd app/ && npm install && cd .. && better-npm-run build:main:prod",
    "start:electron:dev": "cd app/ && npm install --registry=https://registry.npm.taobao.org && cross-env NODE_ENV=development electron --inspect-brk=5858 .",
    "start:electron:prod": "cd app/ && npm install --registry=https://registry.npm.taobao.org && cross-env NODE_ENV=production electron --inspect=5858 .",
    "pack": "cd app/ && npm install --registry=https://registry.npm.taobao.org && cd .. && build --win --ia32 --x64 --mac"
  },
  "betterScripts": {
    "build:renderer:dev": {
      "command": "webpack-dev-server --config webpack.config.renderer.dev.js --progress --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:renderer:prod": {
      "command": "webpack --config webpack.config.renderer.prod.js --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:main:prod": {
      "command": "webpack --config webpack.config.main.js --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:main:dev": {
      "command": "webpack --config webpack.config.main.js --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "build": {
    "appId": "Electron_Scaffold",
    "productName": "Electron_Scaffold",
    "artifactName": "${productName}-${version}-${arch}.${ext}",
    "asar": true,
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost",
        "channel": "latest"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 10,
          "y": 100
        },
        {
          "x": 200,
          "y": 100,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "artifactName": "${productName}-Setup-${arch}-${version}.${ext}"
    }
  },
  "dependencies": {
    "antd": "^2.13.6",
    "babel-runtime": "6.20.0",
    "better-react-spinkit": "^2.0.0",
    "classnames": "2.2.5",
    "core-js": "^2.5.1",
    "es6-promise": "4.0.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "node-fetch": "^1.7.3",
    "nprogress": "0.2.0",
    "prop-types": "^15.5.10",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.6",
    "recompose": "0.26.0",
    "redux": "3.6.0",
    "redux-actions": "1.2.0",
    "reselect": "3.0.0",
    "shallow-equals": "github:joshduck/shallow-equals"
  },
  "devDependencies": {
    "app-module-path": "2.1.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-exponentiation-operator": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "babel-runtime": "^6.20.0",
    "better-npm-run": "0.0.15",
    "body-parser": "1.15.2",
    "browser-sync": "2.18.5",
    "chai": "3.5.0",
    "chai-stats": "0.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "cors": "2.8.1",
    "cross-env": "^5.1.1",
    "css-loader": "0.28.7",
    "devtron": "1.4.0",
    "electron": "1.7.9",
    "electron-builder": "19.42.0",
    "electron-builder-squirrel-windows": "15.0.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "express": "4.14.0",
    "express-delay": "0.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "0.5.7",
    "less": "2.7.1",
    "less-loader": "4.0.5",
    "md5": "2.2.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "multer": "1.2.1",
    "nodemon": "1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "2.0.9",
    "proxy-middleware": "0.15.0",
    "qs": "^6.5.1",
    "react-hot-loader": "^3.1.3",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.5",
    "redux-logger": "2.7.4",
    "sinon": "1.17.7",
    "socket.io": "1.7.2",
    "sonarqube-scanner": "^2.0.2",
    "style-loader": "0.19.1",
    "svg-react-loader": "0.3.7",
    "svgo-loader": "1.1.2",
    "url-loader": "0.6.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "2.0.2",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "2.21.0",
    "worker-loader": "^1.0.0"
  }
}
